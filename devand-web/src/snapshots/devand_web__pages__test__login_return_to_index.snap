          <input type="text" tabindex=1 autofocus="autofocus" name="username" id="username" value="" placeholder="Username" autocomplete="off" autocorrect="off" autocapitalize="off" spellcheck="false" />
        </div>

        <div class="pure-control-group">
          <label for="password">Password <small><a href="/password_reset">Forgot password?</a></small></label>
          <input type="password" tabindex=2 name="password" id="password" value="" placeholder="Password" />
        </div>

        <div class="pure-controls">
          <button tabindex=3 type="submit" class="pure-button pure-button-primary">Sign In</button>
        </div>
      </fieldset>
    </form>

    <p>New to DevAndDev? <a href="/join">Create an account</a></p>


